    TextWindow.WriteLine("mystack length: " + Stack.GetCount(s))
    v = Stack.PopValue(s)
    TextWindow.WriteLine("mystack popped value: " + v)
    TextWindow.WriteLine("mystack length: " + Stack.GetCount(s))
    TextWindow.WriteLine("mystack popped value again: " + Stack.Popvalue(s))
    TextWindow.WriteLine("mystack length: " + Stack.GetCount(s))

    TextWindow.WriteLine("invalid stack name pop: " + Stack.Popvalue("asdf"))

